{
  "name": "eleat",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && cp tsconfig.json dist",
    "codegen:graphql": "gql-gen --config graphql-codegen.ts",
    "dev": "ts-node -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "dev:watch": "nodemon",
    "migrate": "prisma migrate deploy",
    "migrate:start": "pnpm run migrate && pnpm run start",
    "start": "TS_NODE_PROJECT=dist/tsconfig.json node -r tsconfig-paths/register dist/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nutrition",
    "food",
    "calories"
  ],
  "author": "Kyle Satti",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "prisma": "^5.22.0",
    "tsconfig-paths": "^4.2.0",
    "utility-types": "^3.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "dotenv": "^16.5.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}